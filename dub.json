  "license": "MIT",
  "dependencies": {
    "poodinis": "~>8.1",
    "proper-d": "~>0.0.2"
  },
  "configurations": [
    {
      "name": "library",
      "targetType": "library"
    },
    {
      "name": "unittest",
      "targetType": "executable",
